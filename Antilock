    delay = tonumber(a)
end)
spawn(function()
    while wait() do
        if psy then
            focusAdd("PhysicalAddButton")
        elseif kag then
            focusAdd("WeaponAddButton")
        elseif dur then
            focusAdd("DurabilityAddButton")
        elseif speeed then
            focusAdd("SpeedAddButton")
        end
        wait(delay)
    end
end)
